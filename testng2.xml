<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="SmokeSuite" parallel="tests" thread-count="5">
	<listeners>
		<listener class-name="test.Listeners"></listener>
	</listeners>
	<test name="SmokeTests" parallel="classes" thread-count="5">
		<parameter name="URL" value="qaclickacademy.com"></parameter>
		<groups>
			<run>
				<include name="Smoke"></include>
			</run>
		</groups>
		<classes>
			<class name="test.day1"></class>
			<class name="test.day2"></class>
			<class name="test.day4"></class>
		</classes>
	</test>
	
	<test name="Test2">
		<parameter name="URL" value="google.com"></parameter>
		<classes>
			<class name="test.day3"></class>
		</classes>
	</test>
</suite>
